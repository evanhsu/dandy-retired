<h1>Listing frequencies</h1>

<table>
  <tr>
    <th>Radio group</th>
    <th>Channel</th>
    <th>Name</th>
    <th>Rx</th>
    <th>Rx tone</th>
    <th>Tx</th>
    <th>Tx tone</th>
    <th>Band</th>
    <th>Repeater location</th>
    <th>Coverage area</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @frequencies.each do |frequency| %>
  <tr>
    <td><%= frequency.radio_group_id %></td>
    <td><%= frequency.channel %></td>
    <td><%= frequency.name %></td>
    <td><%= frequency.rx %></td>
    <td><%= frequency.rx_tone %></td>
    <td><%= frequency.tx %></td>
    <td><%= frequency.tx_tone %></td>
    <td><%= frequency.band %></td>
    <td><%= frequency.repeater_location %></td>
    <td><%= frequency.coverage_area %></td>
    <td><%= link_to 'Show', frequency %></td>
    <td><%= link_to 'Edit', edit_frequency_path(frequency) %></td>
    <td><%= link_to 'Destroy', frequency, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Frequency', new_frequency_path %>
